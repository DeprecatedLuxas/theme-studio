{
  "c@titleBar.activeForeground": {
    "variable": "titleBar.activeForeground",
    "group": {
      "dark": "#CCCCCC",
      "light": "#333333",
      "hc": "#FFFFFF"
    },
    "tab": "palette",
    "category": "Title Bar",
    "description": "Title bar foreground when the window is active."
  },
  "c@titleBar.inactiveForeground": {
    "variable": "titleBar.inactiveForeground",
    "group": {
      "dark": "@transparent(@titleBar.activeForeground, 0.6)",
      "light": "@transparent(@titleBar.activeForeground, 0.6)",
      "hc": null
    },
    "tab": "palette",
    "category": "Title Bar",
    "description": "Title bar foreground when the window is inactive.",
    "action": "InactiveForeground"
  },
  "bg@titleBar.activeBackground": {
    "variable": "titleBar.activeBackground",
    "group": {
      "dark": "#3C3C3C",
      "light": "#DDDDDD",
      "hc": "#000000"
    },
    "tab": "palette",
    "category": "Title Bar",
    "description": "Title bar background when the window is active."
  },
  "bg@titleBar.inactiveBackground": {
    "variable": "titleBar.inactiveBackground",
    "group": {
      "dark": "@transparent(@titleBar.activeBackground, 0.6)",
      "light": "@transparent(@titleBar.activeBackground, 0.6)",
      "hc": null
    },
    "tab": "palette",
    "category": "Title Bar",
    "description": "Title bar background when the window is inactive.",
    "action": "InactiveBackground"
  },
  "bc@titleBar.border": {
    "variable": "titleBar.border",
    "group": {
      "dark": null,
      "light": null,
      "hc": "@contrastBorder"
    },
    "tab": "palette",
    "category": "Title Bar",
    "description": "Title bar border color."
  }
}
